<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="s180859_s198527.mappe2" >

    <uses-permission android:name="android.permission.SEND_SMS"/>
    <uses-permission android:name="android.permission.RECEIVE_BOOT_COMPLETED"/>
    <uses-permission android:name="android.permission.WAKE_LOCK" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >

        <activity
            android:name="s180859_s198527.mappe2.MainActivity"
            android:label="@string/title_activity_main" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="s180859_s198527.mappe2.Contacts"
            android:label="@string/title_activity_contacts"
            android:parentActivityName="s180859_s198527.mappe2.MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="s180859_s198527.mappe2.MainActivity" />
        </activity>

        <activity
            android:name="s180859_s198527.mappe2.RegContact"
            android:label="@string/title_activity_regcontact"
            android:parentActivityName="s180859_s198527.mappe2.Contacts">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="s180859_s198527.mappe2.Contacts" />
        </activity>

        <activity
            android:name="s180859_s198527.mappe2.Settings"
            android:label="Settings"
            android:parentActivityName="s180859_s198527.mappe2.Contacts">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="s180859_s198527.mappe2.Contacts" />
        </activity>

        <!-- Deklarerer Broadcast receiver som kjører på BOOT_COMPLETED -->
        <receiver
            android:name="s180859_s198527.mappe2.SMSReceiver"
            android:enabled="true"
            android:exported="false"
            android:process=":remote">
            <intent-filter>
                <action android:name="android.intent.action.BOOT_COMPLETED"/>
            </intent-filter>
        </receiver>
        <!-- Deklarerer service BOGERN -->
        <service
            android:name="s180859_s198527.mappe2.SMSService"
            android:exported="false"/>
        <service
            android:name="s180859_s198527.mappe2.SMSAlarm"
            android:exported="false"/>
    </application>
</manifest>
